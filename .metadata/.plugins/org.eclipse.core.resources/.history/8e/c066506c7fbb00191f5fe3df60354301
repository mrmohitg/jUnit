//Anonymous Class is in action
class AnonymousClass3
{
int x=10;
static int y=20;
My display(My M)
{
M.show();
return M;
}
public static void main(String...s)
{
AnonymousClass3 o = new AnonymousClass3();
My obj = new My(){public void show(){System.out.println("Hello");}};
o.display(obj);
}
}

interface My
{
void show();
}
/*Output
Hello
*/